{
    "name": "kayako/php-api-library",
    "type": "library",
    "description": "PHP API library for Kayako",
    "homepage": "https://www.kayako.com",
    "license": "proprietary",
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "^2.9.0"
    },
    "suggest": {
        "monolog/monolog": "A powerful implementation of \"psr/log\" that enables debug"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Kayako\\Api\\Client\\": "src/"
        }
    }
}
